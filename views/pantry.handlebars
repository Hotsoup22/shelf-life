
    <div>
        <h3>Hello {{userPantry.name}}!</h3>
    </div>
    <div class="allLists">
        <div class="listContainer">
            <h4>Expired!</h4>
            {{#each userPantry.products as |product|}}
                {{#if (expiredCheck product.expiration_date)}}
                   {{> itemExpired }}
                {{/if}}
            {{/each}}
        </div>
        <div class="listContainer">
            <h4>Still Usable</h4>
            {{#each userPantry.products as |product|}}
                {{#if (weekCheck product.expiration_date)}}
                    {{> itemWeek }} 
                {{/if}}
            {{/each}}
            <div class="listRow">
                <h5 class="thisMonth tag">This Month 9/4/2021</h5>
                <h5>Cereal</h5>
                <h5>Cupboard</h5>
                <h5>Purchased: 8/10/2021</h5>
                <h5 class="deleteX">X</h5>
            </div>
            <div class="listRow">
                <h5 class="noRush tag">No Rush 10/30/2021</h5>
                <h5>Ice Cream</h5>
                <h5>Freezer</h5>
                <h5>Purchased: 8/10/2021</h5>
                <h5 class="deleteX">X</h5>
            </div>
        </div>
    </div>
